        script(src="https://cdn.jsdelivr.net/npm/vega-lite@2.4.3")
        script(src="https://cdn.jsdelivr.net/npm/vega-embed@3.14.0")


        script(src="/js/layout.js", type="text/javascript")
        script(src="/js/validate.js", type="text/javascript")


        block extra-headers


    body#home-layout.img
        nav.nav-top
            div.container
                div.nav-wrapper
                    a(href="/").brand-logo.personnal-yellow VASCO
                    a(href="/", data-target="mobile-nav").sidenav-trigger
                        i.material-icons.personnal-yellow menu
                    ul.right.hide-on-med-and-down
                        li
                            a(href="/").personnal-yellow.nav-size HOME
                        li
                            a(href="/#contact").personnal-yellow.nav-size CONTACT

                ul.sidenav.mobile-nav-color#mobile-nav
                    li
                        a(href="/").personnal-yellow.nav-size HOME
                    li
                        a(href="/#contact").personnal-yellow.nav-size CONTACT
        div.container
            ol.arrows
                li
                    a(href="/importation") IMPORTATION
                li
                    a(href="/selection") SELECTION
                li
                    a(href="/modification").ignore-click MODIFICATION
                li
                    a(href="/exportation").ignore-click EXPORTATION
