extends layout


block extra-headers
    script.

        var VegaGraph = "#{JSON.stringify(keyGraph)}";
        VegaGraph = JSON.parse(VegaGraph.replace(/&quot;/g,'"'));
        var result = "#{JSON.stringify(keyResult)}";
        result = JSON.parse(result.replace(/&quot;/g, '"'));


block content

    div#selection
        div.container
            h1 Selection de la visualisation
            div.row
                div.col.s6
                    div#selection-table
                            table.highlight.responsive-table
                                thead
                                    tr
                                        th Dimensions
                                        th Somme
                                        th Type
                                tbody
                                    each dim in keyDim
                                        tr
                                            td= dim
                div.col.s6
                    div.row
                        div.col.s6
                            div#vis
                        div.col.s6
                            div#vis2
                div.col.s6
                    div.row
                        div.col.s6
                            pre#query
                        div.col.s6
                            pre#spec




        div.row
            div.col.s6
            div.col.s2
                a.btn.waves-effect.waves-light.right(href="/importation") Retour
                    i.material-icons.left keyboard_arrow_left
            div.col.s2
                a.btn.waves-effect.waves-light.right(href="/modification") Suivant
                    i.material-icons.right keyboard_arrow_right


    script(src="/js/selection.js", type="text/javascript")
